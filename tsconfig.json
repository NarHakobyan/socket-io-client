{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": "src",
    "paths": {
      "@actions/*": [
        "app/core/actions/*"
      ],
      "@actions": [
        "app/core/actions"
      ],
      "@store/*": [
        "app/core/store/*"
      ],
      "@store": [
        "app/core/store"
      ],
      "@effects/*": [
        "app/core/effects/*"
      ],
      "@effects": [
        "app/core/effects"
      ],
      "@modules/*": [
        "app/modules/*"
      ],
      "@modules": [
        "app/modules"
      ],
      "@reducers/*": [
        "app/core/reducers/*"
      ],
      "@reducers": [
        "app/core/reducers"
      ],
      "@selectors/*": [
        "app/core/selectors/*"
      ],
      "@services/*": [
        "app/services/*"
      ],
      "@services": [
        "app/services"
      ],
      "@interfaces/*": [
        "app/interfaces/*"
      ],
      "@components/*": [
        "app/components/*"
      ],
      "@components": [
        "app/components"
      ],
      "@models/*": [
        "app/models/*"
      ],
      "@containers/*": [
        "app/containers/*"
      ],
      "@containers": [
        "app/containers"
      ]
    }
  }
}
